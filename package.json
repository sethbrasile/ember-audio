{
  "name": "ember-audio",
  "version": "1.0.0-beta.1",
  "description": "An Ember addon that makes working with the Web Audio API super EZ",
  "keywords": [
    "ember-addon",
    "audio",
    "sound",
    "music",
    "synthesis",
    "soundfont",
    "sampling",
    "media",
    "mp3",
    "wav",
    "ogg",
    "web",
    "audio",
    "api"
  ],
  "repository": "https://github.com/sethbrasile/ember-audio",
  "license": "MIT",
  "author": "Seth Brasile <seth.brasile@gmail.com>",
  "directories": {
    "doc": "tests/dummy/public/docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.0.2",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-prismjs": "^2.1.0",
    "bootstrap": "^3.4.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.5.0",
    "ember-cli": "~4.10.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylus": "^1.0.7",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.1.1",
    "ember-resolver": "^10.0.0",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.3.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.4.3",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.3",
    "prismjs": "^1.29.0",
    "prismjs-glimmer": "^1.1.1",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.75.0",
    "yuidoc-ember-cli-theme": "^1.0.4"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
