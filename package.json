{
  "name": "ember-audio",
  "version": "1.0.0",
  "description": "An Ember addon that makes working with the Web Audio API super EZ",
  "keywords": [
    "ember-addon",
    "audio",
    "sound",
    "music",
    "synthesis",
    "soundfont",
    "sampling",
    "media",
    "mp3",
    "wav",
    "ogg",
    "web",
    "audio",
    "api"
  ],
  "repository": "https://github.com/sethbrasile/ember-audio",
  "license": "MIT",
  "author": "Seth Brasile <seth.brasile@gmail.com>",
  "directories": {
    "doc": "tests/dummy/public/docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "ember-auto-import": "^1.2.20",
    "ember-cli-babel": "^7.4.3",
    "ember-fetch": "^6.4.0",
    "stereo-panner-node": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@ember/jquery": "^0.5.2",
    "@ember/optional-features": "^0.7.0",
    "bootstrap": "^3.4.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.8.0-beta.1",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.0.1",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylus": "^1.0.7",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-cli-uglify": "^2.1.0",
    "ember-cli-yuidoc": "^0.8.8",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-prism": "^0.4.0",
    "ember-qunit": "^4.2.0",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.8.0-beta.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-ember": "^6.1.0",
    "eslint-plugin-node": "^8.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4",
    "yuidoc-ember-cli-theme": "^1.0.4"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "greenkeeper": {
    "ignore": [
      "bootstrap"
    ]
  }
}
